{
    "manifest_version" : 3,
    "name" : "Detecting Incognito",
    "version" : "0.1",
    "action" : {
        "default_popup" : "popup.html",
        "default_icon" : "hello_extensions.png"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens popup.html"
        }
    }
}